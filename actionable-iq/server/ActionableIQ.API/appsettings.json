{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "THIS_IS_A_PLACEHOLDER_AND_SHOULD_BE_OVERRIDDEN_BY_SECRETS_OR_ENV_VARS"
  },
  "JwtSettings": {
    "TokenExpiryMinutes": 60,
    "RefreshTokenExpiryDays": 7
  },
  "GoogleAnalytics": {
    "ApplicationName": "ActionableIQ Analytics",
    "MaxConcurrentRequests": 5
  }
}